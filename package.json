{
  "name": "gitbook-plugin-page-treeview",
  "version": "2.6.7",
  "description": "a gitbook plugin for generating a treeview of each page",
  "engines": {
    "gitbook": ">=3.0.0-pre.0"
  },
  "main": "./lib/index.js",
  "scripts": {
    "test": "make test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aleen42/gitbook-treeview.git"
  },
  "keywords": [
    "gitbook-plugin",
    "gitbook",
    "treeview"
  ],
  "author": "aleen42 <aleen42@vip.qq.com> (http://aleen42.github.io/pc.html)",
  "contributors": [
    "aleen42 <aleen42@vip.qq.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aleen42/gitbook-treeview/issues"
  },
  "homepage": "https://github.com/aleen42/gitbook-treeview#readme",
  "dependencies": {
    "remarkable": "^1.6.2",
    "remove-markdown": "^0.1.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "mocha": "^3.0.2"
  },
  "gitbook": {
    "properties": {
      "copyright": {
        "type": "string",
        "default": "Copyright Â© aleen42",
        "title": "Description for your Copyright"
      },
      "style": {
        "type": "string",
        "default": "markdown",
        "description": "choose your page treeview style (markdown)"
      },
      "minHeaderCount": {
        "type": "string",
        "default": "1",
        "description": "the minimum of headers"
      },
      "minHeaderDeep": {
        "type": "string",
        "default": "1",
        "description": "the minimum of the depth of headers"
      }
    }
  }
}
